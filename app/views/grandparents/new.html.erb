<div class="container">
  <div class="row-justify-content-center">
  <h1>Create A New Grandparent</h1>
    <%= simple_form_for @grandparent do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :location %>
        <%= f.input :image, label: 'Profile image link' %>
        <%= f.submit "Create Grandparent", class: "btn btn-primary" %>
      <% end %>
  </div>
</div>
